@model IEnumerable<_Components_CategoryMenu_Ihtml>

<li class="dropdown">
    <a class="dropdown-toggle" data-toggle="dropdown">Categories<b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach(var category in Model)
         {
            <li>
                <a asp-controller="Item" asp-action="List"
                   asp-route-category="@category.CategoyName">@category.CategoryName</a>
                //siin on viga küsin konsultatsioonis
            </li>
         }
        <li class="divider"></li>
        <li>
            <a asp-controller="Item" asp-action="List" asp-route-category="">View all item</a>
        </li>
    </ul>
</li>
