@model ItemListViewModel

    
    <h1>@Model.CurrentCategory</h1>
    @foreach (var item in Model.Items)
    {
        <div class="col-sm-4 col-lg-4 col-md-4">
            <div class="thumbnail">
            </div>
            <h2>@item.Name</h2>
            <h3>@item.Price.ToString("c")</h3>
            <h4>@item.Category.CategoryName</h4>

        </div>
    }

    