@model WebSite1.ViewModels.ItemListViewModel
    <html>
    <head>
        <meta charset="utf-8"/>
        <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
        <meta name="viewports" content="width=device-width, initial-scale=1"/>
        <title> Shop </title>
    </head>
    <body>
    <h1>@Model.CurrentCategory</h1>
    @foreach (var item in Model.Items)
    {
        <div>
            <h2>@item.Name</h2>
            <h3>@item.Price.ToString("c")</h3>
            <h4>@item.Category.CategoryName</h4>

        </div>
    }

    </body>
    </html>